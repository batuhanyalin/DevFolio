@model List<DevFolioBireysel.Models.TblSkill>
<p class="title-s">Yeteneklerim</p>
@foreach (var item in Model)
{
    <div class="skill-mf">
        <span>@item.SkillTitle</span> <span class="pull-right">@item.SkillValue %</span>
        <div class="progress">
            <div class="progress-bar" role="progressbar" style="width: 85%;" aria-valuenow="85" aria-valuemin="0"
                 aria-valuemax="100"></div>
        </div>
    </div>
}

