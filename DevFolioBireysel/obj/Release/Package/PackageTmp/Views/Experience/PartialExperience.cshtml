@model List<DevFolioBireysel.Models.TblExperience>
<div class="section-counter paralax-mf bg-image" style="background-image: url(/Templates/devfolio-master/img/codecode.jpg)">
    <div class="overlay-mf"></div>
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-sm-3 col-lg-3">
                    <div class="counter-box">
                        <div class="counter-ico">
                            <span class="ico-circle"><i class="@item.Logo"></i></span>
                        </div>
                        <div class="counter-num">
                            <p class="counter">@item.Puan</p>
                            <span class="counter-text">@item.Baslik</span>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>


