@model DevFolioBireysel.Models.TblSkill
@{
    ViewBag.Title = "CreateSkill";
    Layout = "~/Views/AdminLayout/AdminLayout.cshtml";
}

<h2>Yeni Yetenek Giriş</h2>
<br />
<form method="post">
    <label>Yetenek Adı</label>
    @Html.TextBoxFor(x => x.SkillTitle, new {@class="form-control"})
    <br />
    <label>Yetenek Değeri</label>
    @Html.TextBoxFor(x=>x.SkillValue,new {@class="form-control"})
    <br />
    <button class="btn btn-info">Yeteneği Kaydet</button>
</form>

